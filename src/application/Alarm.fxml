<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AlarmController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="alarmName" text="Medication Name" />
            <Label fx:id="alarmDose" text="Dose description" />
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <Button fx:id="takeButton" mnemonicParsing="false" onAction="#takeButtonClick" text="Take!" />
                  <Button fx:id="noButton" mnemonicParsing="false" onAction="#noButtonClick" text="I'll take it later" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
